{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.csr.html", "/public/site.webmanifest", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/cf-fonts/**",
          "/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ],
        "urls": ["https://fonts.googleapis.com/**", "https://fonts.gstatic.com/**"]
      }
    },
    {
      "name": "clubs",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "urls": ["/api/img/clubs/**"]
      }
    },
    {
      "name": "teams",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "urls": ["/api/img/teams/**"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": ["/api/**"],
      "cacheConfig": {
        "maxSize": 0,
        "maxAge": "0u",
        "strategy": "freshness"
      }
    }
  ]
}
