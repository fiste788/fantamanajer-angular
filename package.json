{
  "name": "fantamanajer-angular",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-local": "ng serve --proxy-config=proxy.dev.conf.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npm run lint:scss && npm run prettier && npm run lint:ts",
    "lint:fix": "npm run lint:scss:fix && npm run prettier:fix && npm run lint:ts:fix",
    "lint:ts": "ng lint",
    "lint:ts:fix": "ng lint --fix",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
    "prettier": "prettier --check \"src/**/*.{scss,html}\" --log-level warn",
    "prettier:fix": "prettier --write \"src/**/*.{scss,html}\" --log-level warn",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "dev": "npm run build && wrangler dev",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "extract-mat-icon": "node ./tools/extract-mat-icon.mjs \"src\""
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.0.3",
    "@angular/common": "^20.0.3",
    "@angular/compiler": "^20.0.3",
    "@angular/core": "^20.0.3",
    "@angular/forms": "^20.0.3",
    "@angular/material": "^20.0.3",
    "@angular/platform-browser": "^20.0.3",
    "@angular/platform-server": "^20.0.3",
    "@angular/router": "^20.0.3",
    "@angular/service-worker": "^20.0.3",
    "@angular/ssr": "^20.0.1",
    "@auth0/angular-jwt": "^5.2.0",
    "@ecodev/fab-speed-dial": "^19.0.0",
    "@fontsource-variable/exo-2": "^5.2.6",
    "@github/webauthn-json": "^2.1.1",
    "@ngneat/content-loader": "^7.0.0",
    "angular-file": "^4.0.2",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-builders/custom-esbuild": "^19.1.0",
    "@angular/build": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.3",
    "@types/html-minifier": "^4.0.5",
    "@types/node": "^22.15.31",
    "angular-eslint": "^20.0.0",
    "browserslist": "^4.25.0",
    "eslint": "^9.28.0",
    "eslint-config-angular-strict": "^1.1.5",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-rxjs-x": "^0.7.5",
    "eslint-plugin-unicorn": "^59.0.1",
    "html-minifier": "^4.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "stylelint": "^16.20.0",
    "stylelint-browser-compat": "^1.0.0-beta.163",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.12.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0",
    "wrangler": "^4.19.2"
  },
  "volta": {
    "node": "22.16.0"
  }
}
