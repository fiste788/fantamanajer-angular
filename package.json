{
  "name": "fantamanajer-angular",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-local": "ng serve --proxy-config=proxy.dev.conf.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npm run lint:scss && npm run prettier && npm run lint:ts",
    "lint:fix": "npm run lint:scss:fix && npm run prettier:fix && npm run lint:ts:fix",
    "lint:ts": "ng lint",
    "lint:ts:fix": "ng lint --fix",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
    "prettier": "prettier --check \"src/**/*.{scss,html}\" --log-level warn",
    "prettier:fix": "prettier --write \"src/**/*.{scss,html}\" --log-level warn",
    "process": "node ./tools/copy-files.mjs",
    "build": "ng build && npm run process",
    "build:stats": "ng build --stats-json",
    "dev": "npm run build && wrangler pages dev",
    "deploy": "npm run build && wrangler pages deploy",
    "type-check": "wrangler types --experimental-include-runtime && tsc",
    "extract-mat-icon": "node ./tools/extract-mat-icon.mjs \"src\""
  },
  "browserslist": [
    ">0.7%",
    "not dead"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.3",
    "@angular/cdk": "^19.1.1",
    "@angular/common": "^19.1.3",
    "@angular/compiler": "^19.1.3",
    "@angular/core": "^19.1.3",
    "@angular/forms": "^19.1.3",
    "@angular/material": "^19.1.1",
    "@angular/platform-browser": "^19.1.3",
    "@angular/platform-browser-dynamic": "^19.1.3",
    "@angular/platform-server": "^19.1.3",
    "@angular/router": "^19.1.3",
    "@angular/service-worker": "^19.1.3",
    "@angular/ssr": "^19.1.4",
    "@auth0/angular-jwt": "^5.2.0",
    "@ecodev/fab-speed-dial": "^17.0.0",
    "@github/webauthn-json": "^2.1.1",
    "@ngneat/content-loader": "^7.0.0",
    "angular-file": "^4.0.2",
    "rxjs": "7.8.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-builders/custom-esbuild": "^18.0.0",
    "@angular-devkit/build-angular": "^19.1.4",
    "@angular/cli": "^19.1.4",
    "@angular/compiler-cli": "^19.1.3",
    "@relative-ci/agent": "^4.2.14",
    "@types/html-minifier": "^4.0.5",
    "@types/node": "^20.17.16",
    "angular-eslint": "^19.0.2",
    "browserslist": "^4.24.4",
    "eslint": "^9.18.0",
    "eslint-config-angular-strict": "^1.1.5",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-rxjs-x": "^0.6.2",
    "eslint-plugin-unicorn": "56.0.1",
    "html-minifier": "^4.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "stylelint": "^16.13.2",
    "stylelint-browser-compat": "^1.0.0-beta.145",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-prettier": "^5.0.2",
    "stylelint-scss": "^6.10.1",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.21.0",
    "wrangler": "^3.105.0"
  },
  "volta": {
    "node": "22.11.0"
  }
}
