{
  "name": "fantamanajer-angular",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-local": "ng serve --proxy-config=proxy.dev.conf.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npm run lint:scss && npm run prettier && npm run lint:ts",
    "lint:fix": "npm run lint:scss:fix && npm run prettier:fix && npm run lint:ts:fix",
    "lint:ts": "ng lint",
    "lint:ts:fix": "ng lint --fix",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
    "prettier": "prettier --check \"src/**/*.{scss,html}\" --log-level warn",
    "prettier:fix": "prettier --write \"src/**/*.{scss,html}\" --log-level warn",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "dev": "npm run build && wrangler dev",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "extract-mat-icon": "node ./tools/extract-mat-icon.mjs \"src\""
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.1.6",
    "@angular/common": "^20.1.7",
    "@angular/compiler": "^20.1.7",
    "@angular/core": "^20.1.7",
    "@angular/forms": "^20.1.7",
    "@angular/material": "^20.1.6",
    "@angular/platform-browser": "^20.1.7",
    "@angular/platform-server": "^20.1.7",
    "@angular/router": "^20.1.7",
    "@angular/service-worker": "^20.1.7",
    "@angular/ssr": "^20.1.6",
    "@auth0/angular-jwt": "^5.2.0",
    "@ecodev/fab-speed-dial": "^19.0.0",
    "@fontsource-variable/exo-2": "^5.2.6",
    "@github/webauthn-json": "^2.1.1",
    "@ngneat/content-loader": "^7.0.0",
    "angular-file": "^4.0.2",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-builders/custom-esbuild": "^20.0.0",
    "@angular/build": "^20.1.6",
    "@angular/cli": "^20.1.6",
    "@angular/compiler-cli": "^20.1.7",
    "@types/html-minifier": "^4.0.5",
    "@types/node": "^22.17.2",
    "angular-eslint": "^20.1.1",
    "browserslist": "^4.25.2",
    "eslint": "^9.33.0",
    "eslint-config-angular-strict": "^1.1.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-rxjs-x": "^0.7.7",
    "eslint-plugin-unicorn": "^60.0.0",
    "html-minifier": "^4.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "stylelint": "^16.23.1",
    "stylelint-browser-compat": "^1.0.0-beta.173",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-plugin-use-baseline": "^0.8.1",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.12.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.1",
    "wrangler": "^4.30.0"
  },
  "volta": {
    "node": "22.16.0"
  }
}
