  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run build && wrangler dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npm run lint:scss && npm run prettier && npm run lint:ts",
    "lint:fix": "npm run lint:scss:fix && npm run prettier:fix && npm run lint:ts:fix",
    "lint:ts": "ng lint",
    "lint:ts:fix": "ng lint --fix",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
    "prettier": "prettier --check \"src/**/*.{scss,html}\" --loglevel warn",
    "prettier:fix": "prettier --write \"src/**/*.{scss,html}\" --loglevel warn",
    "build:stats": "ng build --stats-json",
    "serve:ssr:fantamanajer-angular": "node dist/server/server.mjs",
    "process": "node ./tools/alter-polyfills.mjs",
    "build": "ng build && npm run process",
    "deploy": "npm run build && wrangler deploy"
  },
  "private": true,
