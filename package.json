{
  "name": "fantamanajer-angular",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npm run lint:scss && npm run prettier && npm run lint:ts",
    "lint:fix": "npm run lint:scss:fix && npm run prettier:fix && npm run lint:ts:fix",
    "lint:ts": "ng lint",
    "lint:ts:fix": "ng lint --fix",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
    "prettier": "prettier --check \"src/**/*.{scss,html}\" --log-level warn",
    "prettier:fix": "prettier --write \"src/**/*.{scss,html}\" --log-level warn",
    "process": "node ./tools/copy-files.mjs",
    "build": "ng build && npm run process",
    "build:stats": "ng build --stats-json",
    "dev": "npm run build && wrangler pages dev",
    "deploy": "npm run build && wrangler pages deploy",
    "type-check": "wrangler types --experimental-include-runtime && tsc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.1",
    "@angular/cdk": "^19.0.1",
    "@angular/common": "^19.0.1",
    "@angular/compiler": "^19.0.1",
    "@angular/core": "^19.0.1",
    "@angular/forms": "^19.0.1",
    "@angular/material": "^19.0.1",
    "@angular/platform-browser": "^19.0.1",
    "@angular/platform-browser-dynamic": "^19.0.1",
    "@angular/platform-server": "^19.0.1",
    "@angular/router": "^19.0.1",
    "@angular/service-worker": "^19.0.1",
    "@angular/ssr": "^19.0.2",
    "@auth0/angular-jwt": "^5.2.0",
    "@ecodev/fab-speed-dial": "^17.0.0",
    "@github/webauthn-json": "^2.1.1",
    "@ngneat/content-loader": "^7.0.0",
    "angular-file": "^4.0.2",
    "rxjs": "7.8.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-builders/custom-esbuild": "^18.0.0",
    "@angular-devkit/build-angular": "^19.0.2",
    "@angular/cli": "^19.0.2",
    "@angular/compiler-cli": "^19.0.1",
    "@relative-ci/agent": "^4.2.14",
    "@types/html-minifier": "^4.0.5",
    "@types/node": "^20.17.9",
    "angular-eslint": "19.0.0",
    "eslint": "^9.16.0",
    "eslint-config-angular-strict": "^1.1.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-rxjs-x": "^0.5.0",
    "eslint-plugin-unicorn": "56.0.1",
    "html-minifier": "^4.0.0",
    "prettier": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "stylelint": "^16.11.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-prettier": "^5.0.2",
    "stylelint-scss": "^6.10.0",
    "tailwindcss": "^3.4.15",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.17.0",
    "wrangler": "^3.91.0"
  },
  "volta": {
    "node": "22.11.0"
  }
}
